    "IntegrationProjections": ".",
    "IntegrationProjectionsAdmin": "."
  },

  "DataDog": {
    "Enabled": false,
    "Debug": false,
    "ServiceName": "streetname-registry-projector-dev"
  },

  "BaseUrl": "https://api.staging-basisregisters.vlaanderen/",

  "Integration": {
    "Namespace": "https://data.vlaanderen.be/id/straatnaam",
    "Enabled": false
  },

  "Extract": {
    "DataVlaanderenNamespace": "https://data.vlaanderen.be/id/straatnaam"
  },

  "Elastic": {
    "Uri": "http://localhost:9200",
    "Username": "",
    "Password": "",
    "ListIndexName": "",
    "ListIndexAlias": "",
    "DebugMode": false
  },

  "DistributedLock": {
    "Region": "eu-west-1",
    "TableName": "__DistributedLocks__",
    "LeasePeriodInMinutes": 5,
    "ThrowOnFailedRenew": true,
    "TerminateApplicationOnFailedRenew": true
  },

  "RetryPolicies": {
    "Wfs": {
      "NumberOfRetries": 5,
      "DelayInSeconds": 2
    },
    "Wms": {
      "NumberOfRetries": 5,
      "DelayInSeconds": 2
    }
  },

  "Caches": {
    "LastChangedList": {
      "MaxErrorCount": 10,
      "MaxErrorTimeInSeconds": 60
    }
  },

  "Cors": [
    "http://localhost:3000",
    "http://localhost:5000"
  ],

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Properties": {
      "Application": "StreetNameRegistry - Projector API",
      "ContainerId": "REPLACE_CONTAINERID"
    }
  }
}
